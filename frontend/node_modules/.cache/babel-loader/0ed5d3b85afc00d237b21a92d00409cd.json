r\n             @input=\"$emit('update:param_y', $event.target.value)\">\r\n    </div>\r\n    <div class=\"background\">\r\n      <p>R:</p>\r\n      <CheckboxChain v-model:checkbox_data=\"checkbox_data\" v-model:param_x=\"r\"\r\n                     checkbox_name=\"rCheckbox\" v-on:undo=\"undoR($event)\"/>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport CheckboxChain from \"@/components/pcomponents/CheckboxChain\";\r\n\r\nexport default {\r\n  name: \"ArgsBlock\",\r\n  components: {\r\n    CheckboxChain\r\n  },\r\n  props: ['param_x', 'param_r', 'param_y'],\r\n  emits: ['update:param_x', 'update:param_r', 'update:param_y'],\r\n  data() {\r\n    return {\r\n      checkbox_data: [-5, -4, -3, -2, -1, 0, 1, 2, 3],\r\n      x: [-5],\r\n      r: [1]\r\n    }\r\n  },\r\n  methods: {\r\n    undoR() {\r\n      this.r = [];\r\n      document.querySelectorAll('input[name=\"rCheckbox\"]').forEach(r => r.checked = false);\r\n    },\r\n    undoX() {\r\n      this.x = [];\r\n      document.querySelectorAll('input[name=\"xCheckbox\"]').forEach(x => x.checked = false);\r\n    }\r\n  },\r\n  watch: {\r\n    r(val) {\r\n      if (val.length > 0 && !isNaN(val[0])) {\r\n        this.$emit('update:param_r', val);\r\n      }\r\n    },\r\n    x(val) {\r\n      if (val.length > 0 && !isNaN(val[0])) {\r\n        this.$emit('update:param_x', val);\r\n      }\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":""}]}