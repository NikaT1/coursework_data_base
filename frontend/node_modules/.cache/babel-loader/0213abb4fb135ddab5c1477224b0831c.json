,CAAA,KAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAE/B,GAFT,EAAiD,SAAjD,CAGG,CAJL,CAUG,EALH,mBAAA,CAIK,IAJL,EAAA,UAAA,EAIK,CAHH,mBAAA,CAES,QAFT,EAES;AAFD,IAAA,KAAK,EAAC,QAEL;AAFc,IAAA,EAAE,EAAC,YAEjB;AAF+B,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,UAAA,IAAA,QAAA,CAAA,UAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAEpC,GAFT,EAA2D,mBAA3D,CAGG,CAJL,CAKG,CAXL,CAYM,CA9BR,CA+BG,CAhCL,CAiCG,CAlCL,CAmCM,CApCR,CAqCG,CAtCL,CAuCG,CAxCL,CAyCM,CAjDR,C","sourcesContent":["<template>\r\n  <table id=\"main-table\">\r\n    <tr>\r\n      <td>\r\n        <table id=\"head-table\">\r\n          <Header/>\r\n        </table>\r\n      </td>\r\n    </tr>\r\n    <tr>\r\n      <td>\r\n        <table id=\"body-table\" class=\"body-table\">\r\n          <tr class=\"background\" id=\"background-tr\">\r\n            <td>\r\n              <table>\r\n                <tr>\r\n                  <td colspan=\"3\" class=\"title\">\r\n                    <h2>Вход в аккаунт</h2>\r\n                  </td>\r\n                </tr>\r\n                <td>Логин:</td>\r\n                <td>\r\n                  <input type=\"text\" id=\"loginInput\" maxlength=\"17\" autocomplete=\"off\"\r\n                         placeholder=\"Введите логин\" v-model.trim=\"login\">\r\n                </td>\r\n                <tr>\r\n                  <td>Пароль:</td>\r\n                  <td>\r\n                    <input type=\"password\" id=\"password\" maxlength=\"17\" autocomplete=\"off\"\r\n                           placeholder=\"Введите пароль\" v-model.trim=\"password\">\r\n                  </td>\r\n                </tr>\r\n                <tr>\r\n                  <td colspan=\"2\">\r\n                    <button class=\"button\" id=\"logIn\" @click=\"logIn\">\r\n                      Войти\r\n                    </button>\r\n                  </td>\r\n                  <td colspan=\"3\">\r\n                    <button class=\"button\" id=\"createUser\" @click=\"createUser\">\r\n                      Создать аккаунт\r\n                    </button>\r\n                  </td>\r\n                </tr>\r\n              </table>\r\n            </td>\r\n          </tr>\r\n        </table>\r\n      </td>\r\n    </tr>\r\n  </table>\r\n</template>\r\n\r\n<script>\r\nimport Header from \"@/components/Header\";\r\nexport default {\r\n  components: {\r\n    Header,\r\n  },\r\n  name: \"Index\",\r\n  data(){\r\n    return{\r\n      login: \"\",\r\n      password: \"\"\r\n    }\r\n  },\r\n  beforeMount() {\r\n    localStorage.removeItem('par');\r\n  },\r\n  methods: {\r\n    createUser(e) {\r\n      e.preventDefault()\r\n      const requestOptions = {\r\n        method: \"POST\",\r\n        headers: {\"Content-Type\": \"application/json\"},\r\n        body: JSON.stringify({username: this.login, password: this.password})\r\n      };\r\n      fetch(\"/api/user/new-user\", requestOptions)\r\n          .then(response => {\r\n            if (response.ok) return response.text();\r\n            else {\r\n              return response.text().then(text => { throw new Error(text) });\r\n            }\r\n          }).then(data => {\r\n        localStorage.setItem(\"par\", data);\r\n        this.$router.push({name: 'main-page'});\r\n      }).catch((e) => {\r\n        localStorage.removeItem(\"par\");\r\n        this.$notify({\r\n          group: \"error\",\r\n          title: 'Ошибка',\r\n          text: e.message,\r\n          type: 'error'\r\n        });\r\n      });\r\n    },\r\n    logIn(e) {\r\n      e.preventDefault()\r\n      const requestOptions = {\r\n        method: \"POST\",\r\n        headers: {\"Content-Type\": \"application/json\"},\r\n        body: JSON.stringify({username: this.login, password: this.password})\r\n      };\r\n      fetch(\"/api/user/user\", requestOptions)\r\n          .then(response => {\r\n            if (response.ok) return response.text();\r\n            else {\r\n              return response.text().then(text => { throw new Error(text) });\r\n            }\r\n          }).then(data => {\r\n        localStorage.setItem(\"par\", data);\r\n        this.$router.push({name: 'main-page'});\r\n      }).catch((e) => {\r\n        localStorage.removeItem(\"par\");\r\n        this.$notify({\r\n          group: 'error',\r\n          title: 'Ошибка',\r\n          text: e.message,\r\n          type: 'error'\r\n        });\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\ninput[type='password'] {\r\n  font-family: cursive;\r\n  display: inline-block;\r\n}\r\n\r\n#body-table{\r\n  height: 40%;\r\n}\r\n\r\n#main-table{\r\n  width: 100%;\r\n}\r\n\r\n.title {\r\n  text-align: center;\r\n}\r\n@media(max-width: 1228px) {\r\n  table {\r\n    font-size: 20px;\r\n    padding: 6px;\r\n    border-radius: 6px;\r\n    border-spacing: 6px;\r\n  }\r\n  button, #password, #loginInput {\r\n    font-size: 10px;\r\n  }\r\n}\r\n\r\n@media(max-width: 892px) {\r\n  table {\r\n    font-size: 16px;\r\n    padding: 4px;\r\n    border-radius: 4px;\r\n    border-spacing: 4px;\r\n  }\r\n  button, #password, #loginInput {\r\n    font-size: 8px;\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}